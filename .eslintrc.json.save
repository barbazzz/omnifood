{
    "env": {
        "browser": true,
        "jquery": true
    },
    "extends": ["eslint:recommended", "standard"],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
